import pandas as pd
from sklearn.linear_model import LinearRegression
from sklearn.ensemble import RandomForestRegressor
from sklearn.metrics import mean_squared_error, r2_score
import numpy as np

# 1. Cargar los datasets
train_df = pd.read_csv('train.csv')
test_df = pd.read_csv('test.csv')

# 2. Definir X e Y para entrenamiento y prueba
# La variable objetivo (Y) es 'log_charges'
X_train = train_df.drop('log_charges', axis=1)
Y_train = train_df['log_charges']

X_test = test_df.drop('log_charges', axis=1)
Y_test = test_df['log_charges']

print("Datos de entrenamiento y prueba cargados y separados.")
